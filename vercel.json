{
    "version": 2,
    "routes": [
      { "src": "/$", "dest": "/views/index.ejs" },
      { "src": "/about$", "dest": "/views/about.ejs" },
      { "src": "/contact$", "dest": "/views/contact.ejs" },
      { "src": "/work$", "dest": "/views/work.ejs" },
      { "src": "/healthcare$", "dest": "/views/Healthcare.ejs" }
    ],
    "builds": [
      { "src": "package.json", "use": "@vercel/node" }
    ]
  }

  {
    "version": 2,
    "builds": [
      {
        "src": "server.js", // or whatever your entry file is
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "index.ejs" },
      { "src": "/about/(.*)", "dest": "views/about.ejs" },
      { "src": "/contact/(.*)", "dest": "views/contact.ejs" },
      // Add more routes as needed
      { "src": "/work/(.*)", "dest": "views/work.ejs" },
      { "src": "/healthcare/(.*)", "dest": "views/healthcare.ejs" }
    ]
  }
  